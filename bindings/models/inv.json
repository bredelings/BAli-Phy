{
    "name": "inv",
    "synonyms": ["INV"],
    "result_type": "DiscreteDist<CTMC<a>>",
    "call": "plus_inv(@p_inv,@submodel)",
    "import": ["SModel"],
    "args": [
        {
            "name": "submodel",
            "type": "DiscreteDist<CTMC<a>>"
        },
        {
            "name": "p_inv",
            "type": "Double",
            "default_value": "~uniform(0,1)",
            "description": "The fraction of invariable sites."
        }
    ],
    "title": "Invariable sites model",
    "examples": ["hky85 +> inv",
		 "gtr +> Rates.gamma +> inv"],
    "extract": "all"
}
