{
    "name": "gtr_sym",
    "title":"The GTR exchangability matrix",
    "result_type": "ExchangeModel<a>",
    "call": "gtr_sym'(@sym,@a)",
    "import": ["SModel"],
    "citation":"Tavare, 1986",
    "args": [
        {
            "arg_name": "sym",
            "arg_type": "List<(String,Double)>",
	    "default_value":"~symmetric_dirichlet_on(letter_pairs(@a),1)",
	    "description":"Symmetric exchangability matrix"
        },
        {
            "arg_name": "a",
            "arg_type": "a",
            "default_value": "get_state(alphabet)"
        }
    ],
    "description": "Symmetric exchangability matrix from GTR",
    "extract": "all"
}
