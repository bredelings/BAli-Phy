{
    "name": "nonEq",
    "title":"Non-equilibrium Markov model",
    "result_type": "CTMC<a>",
    "call": "nonEq'(@initialFreq,@submodel)",
    "import": ["SModel"],
    "args": [
        {
            "name": "submodel",
            "type": "CTMC<a>",
	    "default_value":"nonRev",
	    "description":"rate matrix"
        },
        {
            "name": "initialFreq",
            "type": "List<(String,Double)>",
            "default_value": "~symmetric_dirichlet_on(letters(@a),1)",
	    "description": "Initial letter frequencies"

        },
	{
            "name": "a",
            "type": "a",
            "default_value": "get_state(alphabet)",
	    "description": "The alphabet"
        }
    ],
    "description": "A markov model with initial letter frequencies different from the equilibrium frequencies.  Requires a rooted tree.\n\nNote that even reversible models are directional if the initial frequencies are the equilibrium frequencues.",
    "examples": [
	"nonEq",
	"gtr +> nonEq",
	"nonRev +> nonEq",
	"nonRev +> nonEq +> Rates.gamma(4) +> inv"
    ],
    "see": ["gtr","nonRev"],
    "extract": "all"
}
