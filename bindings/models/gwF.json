{
    "name": "gwF",
    "result_type": "CTMC<a>",
    "call": "plus_gwf'(@pi,@f,@submodel)",
    "import": ["SModel"],
    "args": [
        {
            "name": "submodel",
            "type": "ExchangeModel<a>",
            "description": "Symmetric (exchangability) matrix"
        },
        {
            "name": "pi",
            "type": "List<(String,Double)>",
            "default_value": "~symmetric_dirichlet_on(letters(getAlphabet(@submodel)),1.0)"
        },
        {
            "name": "f",
            "type": "Double",
            "default_value": "~uniform(0,1)"
        }
    ],
    "extract": "all"
}
