{
    "name": "f1x4",
    "synonyms": [ "F1x4" ],
    "result_type": "CTMC<a>",
    "constraints": "Triplets<a,b>",
    "call": "plus_f1x4(@pi,@submodel)",
    "import": ["SModel"],
    "args": [
	{
	    "name": "submodel",
	    "type": "ExchangeModel<a>",
	    "description": "Symmetric (exchangeability) matrix"
	},
        {
            "name": "pi",
            "type": "List<(String,Double)>",
            "default_value": "~symmetric_dirichlet_on(letters(getNucleotides(@a)),1)",
	    "description": "Nucleotide frequencies"
        },
        {
            "name": "a",
            "type": "a",
            "default_value":"getAlphabet(@submodel)",
	    "description": "The (triplets) alphabet"
        }
    ],
    "title": "F1x4 codon frequencies",
    "description": "The F1x4 model describes codon frequencies pi' in terms of nucleotide frequencies pi:\n   pi'[ijk] = pi[i]*pi[l]*pi[k].",
    "examples": ["gy94 +> f1x4"],
    "see": ["f3x4","gy94"],
    "extract": "all"
}
