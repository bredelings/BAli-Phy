{
    "name": "logNormal",
    "result_type": "Distribution<Double>",
    "call": "logNormal(@lmu,@lsigma)",
    "import": ["Probability"],
    "args": [
        {
            "name": "lmu",
            "type": "Double",
	    "description": "The mean parameter (log scale)"
        },
        {
            "name": "lsigma",
            "type": "Double",
	    "description": "The standard deviation parameter (log scale)"
        }
    ],
    "title": "The log-normal distribution",
    "examples": ["~logNormal(0,1)","~iid(10, logNormal(0,1))"],
    "description": "A variable X is log-normal if log(X) is normal."
}
