{
    "name": "uniform_rooted_tree",
    "result_type": "Distribution<Tree<Lengths>>",
    "call": "uniformRootedTree(@taxa,@branch_length_dist)",
    "import": ["Probability"],
    "title": "The uniform_topology distribution on trees",
    "args": [
        {
            "name": "taxa",
            "type": "List<Text>",
	    "description": "The taxa for the tree"
        },
	{
	    "name": "branch_length_dist",
	    "type": "Distribution<Double>",
	    "description": "The distribution for branch lengths",
	    "default_value": "gamma(0.5,1/length(@taxa))"
	}
    ]
}
