{
    "name": "iid_on",
    "result_type": "Distribution<List<(a,b)>>",
    "call": "iidOn(@xs,@dist)",
    "import": ["Probability"],
    "args": [
        {
            "name": "xs",
            "type": "List<a>",
	    "description": "The number of samples"
        },
        {
            "name": "dist",
            "type": "Distribution<b>",
	    "description": "The distribution to sample from"
        }
    ],
    "title": "The Independent and Identically Distributed (i.i.d.) distribution",
    "examples": ["~iid_on([1,2,3],normal(0,1))    // A list of 3 normal(0,1) random variables"]
}
