{
    "name": "iid",
    "result_type": "Distribution<List<a>>",
    "call": "iid(@n,@dist)",
    "import": ["Probability"],
    "args": [
        {
            "name": "n",
            "type": "Int",
	    "description": "The number of samples"
        },
        {
            "name": "dist",
            "type": "Distribution<a>",
	    "description": "The distribution to sample from"
        }
    ],
    "title": "The Independent and Identically Distributed (i.i.d.) distribution",
    "examples": ["~iid(10, normal(0,1))    // A list of 10 normal(0,1) random variables"]
}
