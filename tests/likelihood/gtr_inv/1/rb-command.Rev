D <- readDiscreteCharacterData(datadir + "/Primates.nex")
tau <- readTrees(datadir + "/primates.tree")[1]
pi    <- simplex([0.1,0.2,0.4,0.3])
kappa <- 2
Q := fnHKY(kappa,pi)
shape <- 0.5
seq ~ dnPhyloCTMC(tree=tau, Q=Q, pInv = 0.1, branchRates=1.0, type="DNA")
seq.clamp(D)
write("likelihood = ",seq.lnProbability())
