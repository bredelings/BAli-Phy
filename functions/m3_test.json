{
    "name": "m3_test",
    "synonyms": ["M3_Test"],
    "result_type": "MixtureModel[Codons[a]]",
    "call": "SModel.m3_test[submodel,ps,omegas,posP,posW,posSelection]",
    "args": [
        {
            "arg_name": "submodel",
            "arg_type": "Function[Double,RevCTMC[a]]",
            "default_value": "function[w,gy94[omega=w]]",
	    "description": "The model, as a function of dN/dS"
        },
        {
            "arg_name": "ps",
            "arg_type": "List[Double]",
            "default_value": "~dirichlet[@n,2]"
        },
        {
            "arg_name": "omegas",
            "arg_type": "List[Double]",
            "default_value": "~iid[@n,uniform[0,1]]"
        },
        {
            "arg_name": "posP",
            "arg_type": "Double",
            "default_value": "~beta[1,10]"
        },
        {
            "arg_name": "posW",
            "arg_type": "Double",
            "default_value": "~log_gamma[4,0.25]"
        },
        {
            "arg_name": "posSelection",
            "arg_type": "Int",
            "default_value": "~bernoulli[0.5]"
        },
        {
            "arg_name": "n",
            "arg_type": "Int",
            "default_value": "4"
        }
    ],
    "extract": "all"
}
