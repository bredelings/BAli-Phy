{
    "name": "gy94",
    "synonyms": ["M0"],
    "result_type": "RevCTMC[Codons[a]]",
    "constraints": ["Nucleotides[a]"],
    "title": "The Goldman & Yang (1994) model of dN\/dS",
    "call": "SModel.Codons.gy94'[kappa,omega,pi,a]",
    "args": [
        {
            "arg_name": "kappa",
            "arg_type": "Double",
            "default_value": "~log_normal[log[2],0.25]",
            "description": "Transition\/transversion ratio"
        },
        {
            "arg_name": "omega",
            "arg_type": "Double",
            "default_value": "~uniform[0,1]",
            "description": "Relative rate of non-synonymous changes relative to synonymous changes"
        },
        {
            "arg_name": "pi",
            "arg_type": "List[Pair[String,Double]]",
            "default_value": "~dirichlet_on[letters[@a],1]",
	    "description": "Letter frequencies"
        },
        {
            "arg_name": "a",
            "arg_type": "a",
	    "default_value": "getAlphabet",
	    "description": "The codon alphabet"
        }
    ],
