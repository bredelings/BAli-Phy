{
    "name": "m8",
    "synonyms": ["M8"],
    "result_type": "MixtureModel[Codons[a]]",
    "call": "SModel.m8[submodel,mu,gamma,n,posP,posW]",
    "args": [
        {
            "arg_name": "n",
            "arg_type": "Int",
            "default_value": "4"
        },
        {
            "arg_name": "submodel",
            "arg_type": "Function[Double,RevCTMC[a]]",
            "default_value": "function[w,gy94[omega=w]]",
	    "description": "The model, as a function of dN/dS"
        },
        {
            "arg_name": "mu",
            "arg_type": "Double",
            "default_value": "~uniform[0,1]"
        },
        {
            "arg_name": "gamma",
            "arg_type": "Double",
            "default_value": "~beta[1,10]"
        },
        {
            "arg_name": "posP",
            "arg_type": "Double",
            "default_value": "~beta[1,10]"
        },
        {
            "arg_name": "posW",
            "arg_type": "Double",
            "default_value": "~log_gamma[4,0.25]"
        }
    ],
    "extract": "all"
}
